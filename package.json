{
  "name": "@webqit/stateful-js",
  "title": "Stateful JS",
  "description": "Runtime extension to JavaScript that let's us do Imperative Reactive Programming (IRP) in the very language.",
  "keywords": [
    "fine-grained reactivity",
    "js-runner",
    "js-parser"
  ],
  "homepage": "https://webqit.io/tooling/stateful-js",
  "version": "3.0.27",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webqit/stateful-js.git"
  },
  "bugs": {
    "url": "https://github.com/webqit/stateful-js/issues"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.js",
    "./async": "./src/index.async.js",
    "./params": "./src/params.js"
  },
  "scripts": {
    "test": "mocha --extension .test.js --exit",
    "test:coverage": "c8 --reporter=text-lcov npm run test | coveralls",
    "build": "esbuild main=src/api.global.js main.async=src/api.global.async.js compiler=src/api.compiler.global.js --bundle --minify --external:./src/compiler --sourcemap --outdir=dist",
    "preversion": "npm run test && npm run build && git add -A dist",
    "postversion": "npm publish",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "@webqit/observer": "^2.2.9",
    "acorn": "^8.8.1",
    "astring": "^1.8.3"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "esbuild": "^0.14.42",
    "mocha": "^9.0.2",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "author": "Oxford Harrison <oxharris.dev@gmail.com>",
  "maintainers": [
    "Oxford Harrison <oxharris.dev@gmail.com>"
  ],
  "contributors": [],
  "funding": {
    "type": "patreon",
    "url": "https://patreon.com/ox_harris"
  },
  "badges": {
    "list": [
      "npmversion",
      "npmdownloads",
      "patreon"
    ],
    "config": {
      "patreonUsername": "ox_harris",
      "githubUsername": "webqit",
      "githubRepository": "stateful-js",
      "githubSlug": "webqit/stateful-js",
      "npmPackageName": "@webqit/stateful-js"
    }
  }
}
